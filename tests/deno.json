    "tailwindcss/plugin": "npm:tailwindcss@3.4.17/plugin.js",
    "tiny-secp256k1": "npm:tiny-secp256k1@2.2.3",
    "js-beautify": "https://esm.sh/js-beautify@1.15.4",
