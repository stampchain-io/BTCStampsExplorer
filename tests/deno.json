    "swiper/": "https://esm.sh/swiper@11.1.14/",
    "swiper": "https://esm.sh/swiper@11.1.14",
    "sweetalert2": "https://esm.sh/sweetalert2@11.15.10",
    "tailwindcss": "npm:tailwindcss@3.4.17",
