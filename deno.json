{
  "lock": false,
  "lockfile": "deno.lock",
  "tasks": {
    "start": "deno task check_version && deno cache --node-modules-dir deps.ts main.ts server/middleware/openapiValidator.ts && deno run -A main.ts",
    "dev": "deno run -A --watch=routes/,islands/,components/,lib/,server/ --watch-exclude=_fresh/,node_modules/,coverage/,tmp/,dist/,fresh.gen.ts --no-clear-screen dev.ts --host",
    "dev:frontend": "deno run -A --watch=routes/,islands/,components/,static/,tailwind.config.ts --watch-exclude=_fresh/,node_modules/,coverage/,tmp/,dist/,server/,lib/server/ --no-clear-screen dev.ts --host",
    "dev:clean": "deno task kill && deno task dev",
    "kill": "./scripts/kill-servers.sh",
    "kill:safe": "./scripts/kill-servers-safe.sh",
    "check:ports": "deno run --allow-net --allow-run scripts/check-ports.ts",
    "build": "deno cache --node-modules-dir deps.ts main.ts server/middleware/openapiValidator.ts && deno run -A main.ts build",
    "build:fast": "deno cache --node-modules-dir deps.ts main.ts server/middleware/openapiValidator.ts && deno run -A --no-check main.ts build",
    "fakebuild": "echo 'fake build'",
    "preview": "deno run -A main.ts",
    "check": "deno fmt --check . && deno lint --quiet && deno check .",
    "check:fmt": "deno fmt --check .",
    "check:lint": "deno lint --quiet",
    "check:all": "deno task check && deno task check:imports && deno task check:ssr && deno task check:responses",
    "validate": "deno fmt && deno lint --quiet && deno check . && npm run validate:schema && deno task check:imports && deno task check:ssr && deno task check:responses && deno task build && deno task test:unit",
    "validate:ci": "deno task check && deno task check:imports && deno task check:ssr && deno task check:responses && deno task test:unit:coverage",
    "validate:quick": "deno task check && deno task check:imports:report",
    "validate:lenient": "deno fmt && deno lint --quiet && deno check . && npm run validate:schema || true && deno task check:imports:report && deno run --allow-read --allow-write scripts/detect-ssr-unsafe-navigation.ts || true && deno run --allow-read scripts/check-manual-responses.ts || true && deno task build && deno task test:unit",
    "check:imports": "deno run --allow-read --allow-write --allow-run --allow-env scripts/validate-import-patterns.ts --ci --strict",
    "check:imports:report": "deno run --allow-read --allow-write --allow-run --allow-env scripts/validate-import-patterns.ts || true",
    "check:ssr": "deno run --allow-read --allow-write scripts/detect-ssr-unsafe-navigation.ts",
    "check:responses": "deno run --allow-read scripts/check-manual-responses.ts",
    "check:boundary": "bash ./scripts/check-boundary.sh",
    "test:unit": "cd tests && DENO_ENV=test SKIP_REDIS_CONNECTION=true deno test --allow-all unit/ --no-check --parallel",
    "test:unit:coverage": "cd tests && DENO_ENV=test SKIP_REDIS_CONNECTION=true deno test --allow-all unit/ --coverage=../coverage/ --no-check && cd .. && deno coverage coverage/ --exclude='tests/' --exclude='node_modules/' --exclude='_fresh/'",
    "test:integration": "cd tests && DENO_ENV=test SKIP_REDIS_CONNECTION=true deno test --allow-all integration/ --no-check",
    "test:integration:ci": "cd tests && DENO_ENV=test deno test --allow-all integration/databaseManager.integration.test.ts --no-check --coverage=../coverage/",
    "test:integration:watch": "cd tests && DENO_ENV=test SKIP_REDIS_CONNECTION=true deno test --allow-all integration/ --no-check --watch",
    "test:api": "docker-compose -f docker-compose.test.yml run --rm newman",
    "test:api:performance": "docker-compose -f docker-compose.test.yml run --rm newman-performance",
    "test:api:all": "deno task test:api && deno task test:api:performance",
    "test:api:schema": "./scripts/validate-openapi-schema.sh",
    "smoke:test": "BASE_URL=${BASE_URL:-https://stampchain.io} newman run tests/postman/collections/smoke-tests.json --environment-var baseUrl=$BASE_URL --reporters html,json --reporter-html-export ./reports/smoke-test-$(date +%Y%m%d_%H%M%S).html --reporter-json-export ./reports/smoke-test-$(date +%Y%m%d_%H%M%S).json --bail",
    "load:test": "./scripts/load-testing/run-load-tests.sh ${LOAD_TEST_TYPE:-smoke}",
    "monitor:memory": "deno run --allow-net --allow-env scripts/monitorMemory.ts --url=${MONITOR_URL:-http://localhost:8000}",
    "deploy:validate": "deno run --allow-all scripts/deployment/production-readiness-gates.ts",
    "deploy:pre-flight": "BASE_URL=${BASE_URL:-https://stampchain.io} DEPLOYMENT_ENV=${DEPLOYMENT_ENV:-production} ./scripts/deployment/validate-production-readiness.sh",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "check_version": "deno run --allow-run --allow-env utils/check_deno_version.ts",
    "docs": "deno run --allow-read --allow-write swagger.js",
    "decode": "deno run -A utils/decodeSrc20MsigTx.ts",
    "decode_olga": "deno run -A utils/decodeSrc20OlgaTx.ts",
    "deploy:benchmark": "deno run --allow-all scripts/deployment/performance-benchmarking.ts",
    "deploy:rollback": "deno run --allow-all scripts/deployment/automated-rollback.ts ${ROLLBACK_MODE:---check}",
    "deploy:rollback:test": "deno run --allow-all scripts/deployment/automated-rollback.ts --test",
    "deploy:rollback:check": "deno run --allow-all scripts/deployment/automated-rollback.ts --check",
    "deploy:regression": "deno run --allow-all scripts/deployment/regression-detection.ts ${REGRESSION_MODE:-}",
    "validate:dependencies": "deno run --allow-all scripts/validation/dependency-graph-analyzer.ts",
    "validate:dependencies:mermaid": "deno run --allow-all scripts/validation/dependency-graph-analyzer.ts --format=mermaid",
    "validate:dependencies:json": "deno run --allow-all scripts/validation/dependency-graph-analyzer.ts --format=json",
    "validate:client-server": "deno run --allow-all scripts/validation/dependency-graph-analyzer.ts --check-client-server",
    "test:cross-module": "deno task test:integration"
  },
