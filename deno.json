  "fmt": {
    "include": [
      "**/*.ts",
      "**/*.tsx",
      "**/*.js",
      "**/*.jsx",
      "**/*.json"
    ],
    "exclude": [
      "**/_fresh/**/*",
      "**/server/**/*",
      "**/node_modules/**/*",
      "**/dist/**/*",
      "**/.git/**/*",
      "**/coverage/**/*",
      "**/tmp/**/*",
      "**/.cache/**/*",
      "**/.*/",
      "**/.*",
      "**/build/**/*",
      "**/vendor/**/*",
      "**/*.yml",
      "**/*.yaml",
      "**/schema.yml",
      "**/swagger/**/*.yml",
      "**/swagger/**/*.yaml",
      "**/.redocly.*",
      "**/*.css",
      "**/*.json",
      "**/*.md",
      "**/*.html",
      "**/static/**/*",
      ".vscode/**/*",
      ".idea/**/*",
      ".github/**/*",
      ".git/**/*",
      ".env*",
      "scripts/*.js",
      "scripts/*.cjs",
      "deno.json",
      "node_modules/**/*"
    ],
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": false,
    "proseWrap": "preserve"
  }
}
