{
  "deno.enable": true,
  "deno.lint": true,
  "deno.importMap": "./deno.json",
  "deno.suggest.imports.hosts": {
    "https://deno.land": true,
    "https://cdn.nest.land": true,
    "https://crux.land": true
  },
  "deno.codeLens.test": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "editor.indentSize": 2,
  "editor.tabSize": 2,
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[plaintext]": {
    "editor.formatOnSave": false
  },
  "files.associations": {
    ".cursorrules": "plaintext"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "folder-color.pathColors": [
    {
      "folderPath": "BTCStampsExplorer/routes/",
      "color": "foldercolorizer.color_6699ff"
    },
    {
      "folderPath": "BTCStampsExplorer/routes/[...path].ts/"
    },
    {
      "folderPath": "BTCStampsExplorer/islands/",
      "color": "foldercolorizer.color_9966cc"
    },
    {
      "folderPath": "BTCStampsExplorer/components/",
      "color": "foldercolorizer.color_ff99ff"
    },
    {
      "folderPath": "BTCStampsExplorer/static/img/",
      "color": "foldercolorizer.color_996666"
    },
    {
      "folderPath": "BTCStampsExplorer/static/",
      "color": "foldercolorizer.color_99cc66"
    }
  ]
}
